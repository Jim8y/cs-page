{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../static/sui",
      "uncompressed": "../static/sui/components/",
      "compressed": "../static/sui/components/",
      "themes": "../static/sui/themes/"
    },
    "clean": "/"
  },
  "permission": false,
  "autoInstall": false,
  "rtl": false,
  "components": [
    "reset",
    "site",
    "button",
    "container",
    "divider",
    "header",
    "icon",
    "image",
    "label",
    "list",
    "segment",
    "grid",
    "menu",
    "table",
    "item",
    "card"
  ],
  "version": "2.2.4"
}
